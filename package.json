  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/start",
    "dev": "nodemon",
    "test": "cross-env DATABASE_URL=mongodb://127.0.0.1/poketroc-test node --experimental-vm-modules --experimental-json-modules node_modules/.bin/jest",
